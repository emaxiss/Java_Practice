package homeworks;

public class H6
{
	public boolean isPalindrome(String str)
	{
		str = str.toLowerCase();
		String[] words = str.split(" ");
		String pal = "";
		for (int i = 0; i < words.length; i++)
			pal = pal + words[i];
	}
	public static void main(String[] args)
	{
		String palindrome = "Madam Im Adam";
	}
	
	
}
